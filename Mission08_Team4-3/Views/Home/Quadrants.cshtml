@model IEnumerable<CreateTasks> <!-- The model for the view is a collection of TaskItem -->

@{
    ViewBag.Title = "View Tasks";
}

<h2>View Tasks</h2>

<ul>
    @foreach (var task in Model.Where(t => !t.Completed)) <!-- Display only not completed tasks -->
    {
        <li>
            <strong>@task.Title</strong> -
        Importance: @(task.IsImportant ? "Important" : "Not Important"),
        Urgency: @(task.IsUrgent ? "Urgent" : "Not Urgent")
        </li>
    }
</ul>
